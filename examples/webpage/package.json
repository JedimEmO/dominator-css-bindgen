{
  "name": "dwind-webpage",
  "version": "0.1.0",
  "scripts": {
    "build": "rimraf public/js && rollup --bundleConfigAsCjs --config",
    "build:release": "rimraf public/js && rollup --bundleConfigAsCjs --config rollup.config.release.js",
    "start": "rimraf public/js && rollup --bundleConfigAsCjs --config --watch",
    "start:release": "rimraf public/js && rollup --bundleConfigAsCjs --config rollup.config.release.js --watch"
  },
  "devDependencies": {
    "@wasm-tool/rollup-plugin-rust": "^2.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^2.0.3",
    "rollup-plugin-serve-proxy": "^1.1.4",
    "rollup-plugin-terser": "^7.0.2",
    "fast-glob": "^3.3.2"
  }
}
